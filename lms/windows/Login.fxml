<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="342.0" prefWidth="414.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lms.windows.LoginController">
   <children>
      <TextField fx:id="usernameField" layoutX="66.0" layoutY="103.0" prefHeight="37.0" prefWidth="299.0" promptText="Username">
         <font>
            <Font name="Arial" size="20.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="66.0" layoutY="157.0" prefHeight="37.0" prefWidth="299.0" promptText="Password">
         <font>
            <Font name="Arial" size="20.0" />
         </font>
      </PasswordField>
      <Label layoutX="119.0" layoutY="34.0" prefHeight="37.0" prefWidth="176.0" text="PLEASE LOG IN" textAlignment="CENTER">
         <font>
            <Font name="Arial" size="23.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="164.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleClick" prefHeight="45.0" prefWidth="87.0" text="Login" />
      <Label fx:id="errorLabel" layoutX="102.0" layoutY="204.0" prefHeight="37.0" prefWidth="228.0" textFill="#dd0303" />
   </children>
</AnchorPane>
