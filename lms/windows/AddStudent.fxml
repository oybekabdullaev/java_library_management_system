<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lms.windows.AddStudentController">
   <children>
      <VBox layoutX="10.0" layoutY="14.0" prefHeight="421.0" prefWidth="396.0">
         <children>
            <Label prefHeight="33.0" prefWidth="366.0" text="Add Student" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="Arial Bold" size="24.0" />
               </font>
            </Label>
            <TextField fx:id="login" prefHeight="38.0" prefWidth="359.0" promptText="Login">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="password" prefHeight="38.0" prefWidth="359.0" promptText="Password">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="name" prefHeight="38.0" prefWidth="359.0" promptText="Name">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="surname" prefHeight="38.0" prefWidth="359.0" promptText="Surname">
               <VBox.margin>
                  <Insets bottom="40.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="74.0" prefWidth="396.0">
               <children>
                  <Button fx:id="addStudent" mnemonicParsing="false" onAction="#addStudent" prefHeight="51.0" prefWidth="142.0" text="Add" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
                     <HBox.margin>
                        <Insets right="30.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="Cancel" mnemonicParsing="false" onAction="#cancel" prefHeight="49.0" prefWidth="134.0" text="Cancel" />
               </children>
               <padding>
                  <Insets left="45.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
